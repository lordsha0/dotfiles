#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

# alias ls='ls --color=auto'
# PS1='[\u@\h \W]\$ '

alias nv="nvim"
alias pac="sudo pacman -S"
alias pac-u="sudo pacman -Syu"
alias pac-r="sudo pacman -Rns"
alias mv="mv -iv"
alias cp="cp -iv"
alias power="sudo poweroff"
alias reboot="sudo reboot"
alias untar="tar -xf"
alias cl="clear"
alias \:q="exit"
